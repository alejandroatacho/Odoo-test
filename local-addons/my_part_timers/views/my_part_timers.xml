<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id='my_part_timers_action' model='ir.actions.act_window'>
        <field name='name'>My Part Timers</field>
        <field name='res_model'>my_part_timers</field>
        <field name='view_mode'>tree,form</field>
    </record>
    <menuitem id='my_part_timers_menu' name='My Part Timers' web_icon="my_part_timers,static/description/applogo.png" />
    <menuitem id='my_part_timers_menu_root' name='My Part Timers' parent='my_part_timers_menu' action='my_part_timers_action' />
    <!-- Data records go here -->
    <record id="my_part_timers_view_tree" model="ir.ui.view">
        <field name="name">my.part.timers.view.tree</field>
        <field name="model">my.part.timers</field>
        <field name="arch" type="xml">
            <tree string="My Part Timers">
                <field name="name" />
                <field name="date" />
                <field name="hours" />
                <field name="description" />
            </tree>
        </field>
    </record>
    <record id="my_part_timers_search_view" model="ir.ui.view">
        <field name="name">my.part.timers.search.view</field>
        <field name="model">my.part.timers</field>
        <field name="arch" type="xml">
            <search string="My Part Timers">
                <field name="name" />
                <field name="date" />
                <field name="hours" />
                <field name="description" />
            </search>
        </field>
    </record>
</odoo>